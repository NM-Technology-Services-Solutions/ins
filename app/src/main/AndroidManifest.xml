<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    xmlns:tools="http://schemas.android.com/tools"
    package="co.mz.ins">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logotipo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logotipo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".api.ApiUtils"
        android:usesCleartextTraffic="true">

        <activity android:name=".NotFoundAmostra"></activity>
        <activity android:name=".AmostraSearchResult" />
        <activity android:name=".RequestAmostra" />
        <activity android:name=".AprovarResultado" />
        <activity android:name=".ResultsPageContainer" />
        <activity android:name=".GetAnalysisRequest"/>
        <activity android:name=".GetPatient"/>
        <activity android:name=".LoginPage" />
        <activity android:name=".HomePage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission
        android:name="android.permission.USE_FINGERPRINT"
        tools:ignore="ManifestOrder" />

    <dist:module dist:instant="true" />

</manifest>