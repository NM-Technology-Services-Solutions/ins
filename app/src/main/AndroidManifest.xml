<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    xmlns:tools="http://schemas.android.com/tools"
    package="co.mz.ucmins">


    <application
        android:name="mz.ac.ucmins.api.ApiUtils"
        android:allowBackup="true"
        android:icon="@drawable/logotipo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logotipo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="mz.ac.ucmins.views.NotFoundAmostra" />

        <activity android:name="mz.ac.ucmins.views.AmostraSearchResult" />
        <activity android:name="mz.ac.ucmins.views.RequestAmostra" />
        <activity android:name="mz.ac.ucmins.views.AprovarResultado" />
        <activity android:name="mz.ac.ucmins.views.ResultsPageContainer" />
        <activity android:name="mz.ac.ucmins.views.GetAnalysisRequest" />
        <activity android:name="mz.ac.ucmins.views.GetPatient" />
        <activity android:name="mz.ac.ucmins.views.LoginPage" />
        <activity android:name="mz.ac.ucmins.views.HomePage" />
        <activity android:name="mz.ac.ucmins.views.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_channel_id"
            android:value="default_notification_channel_id"/>
        <service
            android:name="mz.ac.ucmins.services.INSFirebaseService"
            android:enabled="true"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>

        </service>
    </application>
    <uses-permission
        android:name="android.permission.USE_FINGERPRINT"
        tools:ignore="ManifestOrder" />

    <uses-permission android:name="android.permission.INTERNET" />

    <dist:module dist:instant="true" />


    </manifest>